server {                                                                                                                                                     listen 80 default_server;                                                                                                                            listen[::]:80 default_server;                                                                                                                        add_header X-Served-By $HOSTNAME;                                                                                                                                                                                                                                                                         index index.html index.htm index.nginux-debian.html;                                                                                                                                                                                                                                                      error_page 404 /404.html;                                                                                                                            server_name _;                                                                                                                                                                                                                                                                                            location / {                                                                                                                                                 try_files $url $url/ =404;                                                                                                                   }                                                                                                                                            }  


